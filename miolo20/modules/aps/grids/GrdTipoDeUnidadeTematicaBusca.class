class GrdTipoDeUnidadeTematicaBusca extends SGrid {

    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'apstipodeunidadetematica.tipodeunidadetematicaid' => new MGridColumn(_M("Identificador", $module)),
            'apstipodeunidadetematica.descricao' => new MGridColumn(_M("Descrio", $module)),
        );

        parent::__construct($filters, $columns, __CLASS__, array('tipodeunidadetematicaid' => '%apstipodeunidadetematica.tipodeunidadetematicaid%'));
    }

}

