class GrdCSVLogSearch extends SGrid
{
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'bascsvlog.importdate' => new MGridColumn( _M('Data', $module) ),
            'bascsvlog.line' => new MGridColumn( _M('Linha do CSV', $module)),
            'bascsvlog.type' => new MGridColumn( _M('Tipo', $module)),
            'bascsvlog.colname' => new MGridColumn( _M('Coluna', $module) ),
            'bascsvlog.information' => new MGridColumn( _M('Informao', $module) ),
            'bascsvlog.error' => new MGridColumn( _M('Erro', $module) ),
        );

        parent::__construct($filters, $columns, __CLASS__, array('csvlogid' => '%bascsvlog.csvlogid%'));
        
        $this->clearActions();
    }
}
