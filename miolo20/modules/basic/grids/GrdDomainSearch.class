class GrdDomainSearch extends SGrid
{
    public function __construct($filters)
    {
        $module = SAGU::getFileModule(FILE);
        $columns[] = new MGridColumn(_M('Domínio', $module), 'left', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Sequencia', $module), 'right', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Chave', $module), 'left', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Abreviatura', $module), 'left', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Etiqueta', $module), 'left', true, NULL, true, NULL, true);
        
        $primaryKeys = array('domainId' => '%0%', 'sequence' => '%1%');
        
        parent::__construct($filters, $columns, __CLASS__, $primaryKeys);
    }
}


