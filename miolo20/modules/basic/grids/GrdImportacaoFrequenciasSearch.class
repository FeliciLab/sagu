class GrdImportacaoFrequenciasSearch extends SGrid
{
    public function __construct($filters)
    {
        $MIOLO = MIOLO::getInstance();
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'basimportacaofrequencias.importacaofrequenciaid' => new MGridColumn( _M('Cdigo', $module)),
            'basimportacaofrequencias.data' => new MGridColumn( _M('Data da importao', $module)),
            'basimportacaofrequencias.modulodestino' => new MGridColumn( _M('Mdulo de destino', $module)),
            'basimportacaofrequencias.datainicialregistros' => new MGridColumn( _M('Data inicial dos registros', $module)),
            'basimportacaofrequencias.datafinalregistros' => new MGridColumn( _M('Data final dos registros', $module)),
            'basimportacaofrequencias.datageracaoarquivo' => new MGridColumn( _M('Data de gerao do arquivo', $module)),
            'basimportacaofrequencias.horariogeracaoarquivo' => new MGridColumn( _M('Horrio de gerao', $module)),
            
        );        

        parent::__construct($filters, $columns, __CLASS__, array('importacaofrequenciaid' => '%basimportacaofrequencias.importacaofrequenciaid%'));        
        
        $this->clearActions();
        
        $url_visualizar = $MIOLO->getActionURL($module, 'main:config:importacaofrequencias', null, array('importacaofrequenciaid'=>'%basimportacaofrequencias.importacaofrequenciaid%', 'function' => 'insert' ,'event'=>'visualizar_click'));
        $this->addActionIcon(_M('Visualizar registros', $module), 'leaf_.gif', $url_visualizar);
    }
}
?>