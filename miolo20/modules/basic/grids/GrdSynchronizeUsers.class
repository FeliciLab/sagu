class GrdSynchronizeUsers extends SGrid
{
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        $columns = array(
            new MGridColumn( _M('Usuário', $module), 'left', true, null, true, null, true),
            new MGridColumn( _M('Status', $module), 'left' , true, null, true, null, true ) );
        
        parent::__construct($filters, $columns, __CLASS__, $primaryKey);
        
        $this->clearActions();
        $this->pageLength = 9999999;
    }
}
