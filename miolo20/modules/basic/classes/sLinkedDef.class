class sLinkedDef
{
    /**
     * @var string
     */
    private $chave;
    
    /**
     * @var array
     */
    private $forms;
    
    public function getChave() {
        return $this->chave;
    }

    public function setChave($chave) {
        $this->chave = $chave;
        
        return $this;
    }

    public function getForms() {
        return $this->forms;
    }

    public function setForms($forms) {
        $this->forms = $forms;
        
        return $this;
    }

    public function addForm(sLinkedFormDef $form)
    {
        $this->forms[] = $form;
        
        return $this;
    }
}
