class sLinkedFormDef
{
    /**
     * @var string
     */
    private $label;

    /**
     * @var string
     */
    private $action;
    
    /**
     * @var string
     */
    private $module;
    
    /**
     * @var string
     */
    private $tipo;

    /**
     * @var string
     */
    private $gridModule;
    
    /**
     * @var string
     */
    private $gridName;
    
    /**
     * @var boolean
     */
    private $isDisabled;
    
    /**
     *
     * @var array 
     */
    private $argumentos = array();
    
    /**
     * @var boolean
     */
    private $desabilitar;
    
    public function getLabel() {
        return $this->label;
    }

    public function setLabel($label) {
        $this->label = $label;
        
        return $this;
    }

    public function getAction() {
        return $this->action;
    }

    public function setAction($action) {
        $this->action = $action;
        
        return $this;
    }

    public function getModule() {
        return $this->module;
    }

    public function setModule($module) {
        $this->module = $module;
        
        return $this;
    }

    public function getTipo() {
        return $this->tipo;
    }

    public function setTipo($tipo) {
        $this->tipo = $tipo;
        
        return $this;
    }
    
    public function getGridModule() {
        return $this->gridModule;
    }

    public function setGridModule($gridModule) {
        $this->gridModule = $gridModule;
        
        return $this;
    }

    public function getGridName() {
        return $this->gridName;
    }

    public function setGridName($gridName) {
        $this->gridName = $gridName;
        
        return $this;
    }
    
    public function getIsDisabled() {
        return $this->isDisabled;
    }

    public function setIsDisabled($isDisabled) {
        $this->isDisabled = $isDisabled;
        
        return $this;
    }
    
    public function setArgs($args)
    {
        $this->argumentos = $args;        
        return $this;
    }
    
    public function getArgs()
    {
        return $this->argumentos;
    }
    
    public function getDesabilitar()
    {
        return $this->desabilitar;
    }

    public function setDesabilitar($desabilitar)
    {
        $this->desabilitar = $desabilitar;
        
        return $this;
    }
}
?>