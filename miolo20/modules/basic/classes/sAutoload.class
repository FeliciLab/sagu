    /**
     * Funcao chamada pelo miolo (support.inc) 
     */
    public static function SAGUAutoload($className, $homeDir=null, $portal=false)
    {
        $MIOLO = MIOLO::getInstance();
        $lcClass = strtolower($className);
	$ok = false;

        // Executa rotinas de carregamento automatico de business
        if ( !$homeDir )
        {
            $homeDir = $MIOLO->getConf('home.modules');
        }
        
        if ( substr($lcClass, 0, 8) == 'business' )
        {
            $withoutBusiness = str_replace('Business', '', $className);
            $positionBus = strpos( $withoutBusiness , 'Bus');
            $busModule = lcfirst(substr($className, 8, $positionBus));
            $busName = substr($withoutBusiness, $positionBus);
            $file = $homeDir . '/' . $busModule . '/db/' . $busName . '.class';


            if ( file_exists($file) )
            {
                include_once $file;
		$ok = true;
            }
            
            $path = $MIOLO->getModulePath($busModule, 'types.class');

            
            if (file_exists($path))
            {
		if($portal)
		{
		    include_once($path);
		}
		else
		{
		    $MIOLO->Uses('types.class', $busModule);
		}
		$ok = true;
            }
        }
        else
        {

            // Executa rotinas de carregamento automatico de types
            // FIXME Cuidar com performance
            $classModule = SModules::getInstance()->getModuleByPrefix( substr($lcClass, 0, 3) );

            if ( $classModule )
            {
                $file = $homeDir . '/' . $classModule->getName() . '/types/' . ucfirst($className) . '.class';

                if ( file_exists($file) )
                {
                    include_once $file;
		    $ok = true;
                }
                else
                {
                    $typesFile = $MIOLO->getModulePath($classModule->getName(), 'types.class');
                    
                    if ( file_exists($typesFile) )
                    {
                        include_once $typesFile;
			$ok = true;
                    }
                }
            }
        }

      return $ok;

    }
}
