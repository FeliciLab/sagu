    protected $_utilizaNovoPadrao = true;
    
    const LAYOUT_CNAB240 = 'CNAB240';
    const LAYOUT_CNAB400 = 'CNAB400';
    
    const DATA_DDMMAA = 'ddmmyy';
    const DATA_DDMMAAAA = 'ddmmyyyy';
    
    const ENVIO_BANCO = 'B';
    const ENVIO_EMPRESA = 'E';
    
    public $cobrancabancariaid;
    public $descricao;
    public $leiaute;
    public $envio;
    public $formatodata;
