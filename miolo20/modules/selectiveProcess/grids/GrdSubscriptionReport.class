        $module = MIOLO::getCurrentModule();

        $columns = array( new MGridColumn(_M('Inscrio', $module), 'right', true, null, true, null, true),
                          new MGridColumn(_M('Ordem de inscrio', $module), 'right', true, null, false, null, true),
                          new MGridColumn(_M('Cdigo do processo seletivo', $module), 'right', true, null, false, null, true),
                          new MGridColumn(_M('Processo seletivo', $module), 'left', true, null, false, null, true),
                          new MGridColumn(_M('Cdigo da pessoa', $module), 'right', true, null, true, null, true),
                          new MGridColumn(_M('Pessoa', $module), 'left', true, null, true, null, true),
                          new MGridColumn(_M('Opes', $module), 'left', true, null, true, null, true),
                          new MGridColumn(_M('Data de nascimento', $module), 'left', true, null, true, null, true),
                          new MGridColumn(_M('Sexo', $module), 'left', true, null, true, null, true),
                          new MGridColumn(_M('Cidade', $module), 'left', true, null, true, null, true),
                          new MGridColumn(_M('Logradouro', $module), 'left', true, null, true, null, true),
                          new MGridColumn(_M('Bairro', $module), 'left', true, null, true, null, true),
