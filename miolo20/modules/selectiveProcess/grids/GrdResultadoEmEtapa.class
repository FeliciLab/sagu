class GrdResultadoEmEtapa extends SGrid
{
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        $columns = array(
            new MGridColumn(_M('Código', $module), 'right', true, null, true, null, false ),
            new MGridColumn(_M('Nome', $module), 'left',  true, null, true, null, false ),
            new MGridColumn(_M('Status', $module), 'left',  true, null, true, null, false ),
            new MGridColumn(_M('Posição', $module), 'right',  true, null, true, null, false ),            
            new MGridColumn(_M('Pontuação', $module), 'right',  true, null, true, null, false ),
            new MGridColumn(_M('Pontuação na etapa', $module), 'right',  true, null, true, null, false ),
        );

        parent::__construct($filters, $columns, __CLASS__, $primaryKey);
        
        $this->clearActions();
        $this->setClose(null);
    }
}
