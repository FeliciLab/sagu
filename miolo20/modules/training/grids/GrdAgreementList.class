class GrdAgreementList extends SGrid
{
    public function __construct($filters)
    {
        $module = SAGU::getFileModule(__FILE__);

        $columns = array();
        $columns[] = new MGridColumn(_M('Convnio', $module), 'right', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Cdigo conveniada', $module), 'right', true, NULL, false, NULL, true);
        $columns[] = new MGridColumn(_M('Conveniada', $module), 'left', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Incio', $module), 'left', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Fim', $module), 'left', true, NULL, true, NULL, true);

        parent::__construct($filters, $columns, __CLASS__, $primaryKey);

        $this->clearActions();
    }
}
