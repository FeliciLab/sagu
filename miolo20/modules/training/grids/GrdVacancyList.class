class GrdVacancyList extends SGrid
{
    public function __construct($filters)
    {
        $module = SAGU::getFileModule(__FILE__);

        $columns = array();
        $columns[] = new MGridColumn( _M('Cdigo convnio', $module), 'right', true, null, true, null, true );
        $columns[] = new MGridColumn( _M('Instituio', $module), 'left',  true, null, true, null, true );
        $columns[] = new MGridColumn( _M('Curso', $module), 'left', true, null, true, null, true );
        $columns[] = new MGridColumn( _M('Total de vagas', $module), 'left',  true, null, false, null, true );
        $columns[] = new MGridColumn( _M('Vagas', $module), 'right', true, null, true, null, true );
        $columns[] = new MGridColumn( _M('rea', $module), 'left',  true, null, true, null, true );

        parent::__construct($filters, $columns, __CLASS__, $primaryKey);

        $this->clearActions();
    }
}
