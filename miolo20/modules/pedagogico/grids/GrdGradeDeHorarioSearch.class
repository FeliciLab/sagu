class GrdGradeDeHorarioSearch extends SGrid
{
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'acpgradehorario.gradehorarioid' => new MGridColumn( _M('Cdigo', $module)),
            'acpgradehorario.descricao' => new MGridColumn( _M('Descrio', $module)),
            'acpgradehorario.detalhes' => new MGridColumn( _M('Detalhes', $module)),
            'acpgradehorario.ativo' => new MGridColumn( _M('Ativo', $module)),
        );

        parent::__construct($filters, $columns, __CLASS__, array('gradehorarioid' => '%acpgradehorario.gradehorarioid%'));
    }
}
