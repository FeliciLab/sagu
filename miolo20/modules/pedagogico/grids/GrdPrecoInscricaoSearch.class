class GrdPrecoInscricaoSearch extends SGrid
{
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'prcprecocurso.precocursoid' => new MGridColumn( _M('Cdigo', $module)),
            'prcprecocurso._ocorrenciacurso' => new MGridColumn( _M('Ocorrncia de curso', $module)),
            'prcprecocurso._ofertacurso' => new MGridColumn( _M('Oferta de curso', $module)),
            'prcprecocurso._matrizcurriculargrupo' => new MGridColumn( _M('Grupo', $module)),
            'prcprecocurso.datainicial' => new MGridColumn( _M('Data inicial', $module)),
            'prcprecocurso.datafinal' => new MGridColumn( _M('Data final', $module)),
            'prcprecocurso.valoravista' => new MGridColumn( _M('Valor  vista', $module)),
        );

        parent::__construct($filters, $columns, __CLASS__, array('precocursoid' => '%prcprecocurso.precocursoid%'));
    }
}
