class GrdTipoDeCoordenadorSearch extends SGrid
{
    public function __construct($filters)
    {
        $MIOLO = MIOLO::getInstance();
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'acptipodecoordenador.tipodecoordenadorid' => new sGridColumn( null, _M('Código', $module), 'right'),
            'acptipodecoordenador.descricao' => new sGridColumn( null, _M('Descrição', $module), 'left'),
        );
        
        $primaryKey = array('tipodecoordenadorid' => '%acptipodecoordenador.tipodecoordenadorid%');
        
        parent::__construct($filters, $columns, __CLASS__, $primaryKey);
    }
}