class GrdCentroCustoOcorrenciaCursoSearch extends SGrid
{
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'prccentrocustoocorrenciacurso.centrocustoocorrenciacursoid' => new MGridColumn( _M('Cdigo', $module)),
            'prccentrocustoocorrenciacurso._ocorrenciacurso' => new MGridColumn( _M('Ocorrncia de curso', $module)),
            'prccentrocustoocorrenciacurso._centrodecusto' => new MGridColumn( _M('Centro de custo', $module)),
        );

        parent::__construct($filters, $columns, __CLASS__, array('centrocustoocorrenciacursoid' => '%prccentrocustoocorrenciacurso.centrocustoocorrenciacursoid%'));
    }
}
