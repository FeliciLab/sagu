class GrdInscricaoHorarios extends SGrid
{
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'componentecodigo' => new MGridColumn( _M('Cdigo', $module)),
            'componentedesc' => new MGridColumn( _M('Componente curricular', $module)),
            'datainicio' => new MGridColumn( _M('Data inicial', $module), 'center'),
//            'diasemana' => new MGridColumn( _M('Dia da semana', $module), 'left'),
            'horario' => new MGridColumn( _M('Horrio', $module)),
            'professor' => new MGridColumn( _M('Professor', $module)),
//            'sala' => new MGridColumn( _M('Sala', $module)),
        );

        parent::__construct($filters, $columns, __CLASS__, array('codigo' => '%codigo%'));
        $this->clearActions();
    }
}
