class FrmFechamentoDePeriodoContabilBusca extends SSearchForm
{
    protected $campoBuscaGenerica = true;
    
    /**
     * Class constructor
     */
    public function __construct($data)
    {
        $MIOLO  = MIOLO::getInstance();
        $module = MIOLO::getCurrentModule();

        parent::__construct( _M('Localizar', $module), new AccFechamentoDePeriodoContabil() );
    }

    /**
     *  Default method to define fields
     */
    public function defineFields()
    {
        $MIOLO = MIOLO::getInstance();
        $module = MIOLO::getCurrentModule();
        
        parent::defineFields( $fields, $module, 'GrdFechamentoDePeriodoContabilBusca', $validators );
    }
}


