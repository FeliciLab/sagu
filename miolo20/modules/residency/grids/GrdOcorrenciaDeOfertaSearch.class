class GrdOcorrenciaDeOfertaSearch extends SGrid
{
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();
        $columns[] = new MGridColumn(_M('Código', $module), 'right', true, NULL, false, NULL, true);
        $columns[] = new MGridColumn(_M('ofertadoresidenteid', $module), 'right', true, NULL, false, NULL, true);
        $columns[] = new MGridColumn(_M('Data/hora', $module), 'left', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Status', $module), 'left', true, NULL, true, ResOcorrenciaDeOferta::listStatus(), true);
        $columns[] = new MGridColumn(_M('Observações', $module), 'left', true, NULL, true, NULL, true);
        $primaryKeys = array('ocorrenciaDeOfertaId'=>'%0%', );
        parent::__construct($filters, $columns, __CLASS__, $primaryKeys);
        $this->clearActions();
        // Desabilita botão CSV
        $this->showExportAsCSV = false;
        // Desabilita paginação
        $this->pageLength = 0;
    }
}
