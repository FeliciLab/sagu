class GrdEncerrarUnidadeTematica extends SGrid
{
    public function __construct($filters)
    {
        $MIOLO = MIOLO::getInstance();
        $module = MIOLO::getCurrentModule();

        $columns[] = new MGridColumn(_M('ofertaDoResidenteId', $module), 'left', true, NULL, false, NULL, false);
        $columns[] = new MGridColumn(_M('ofertadeunidadetematicaid', $module), 'left', true, NULL, false, NULL, false);
        $columns[] = new MGridColumn(_M('residenteid', $module), 'left', true, NULL, false, NULL, false);
        $columns[] = new MGridColumn(_M('Residente', $module), 'left', true, NULL, true, NULL, false);
        $columns[] = new MGridColumn(_M('Carga horria', $module), 'center', true, NULL, true, NULL, false);
        $columns[] = new MGridColumn(_M('Prtica', $module), 'center', true, NULL, true, NULL, false);
        $columns[] = new MGridColumn(_M('Terico-conceitual (EAD+PRESENCIAL)', $module), 'center', true, NULL, true, NULL, false);
        $columns[] = new MGridColumn(_M('Terico-prtica (CAMPO+NCLEO)', $module), 'center', true, NULL, true, NULL, false);
        $columns[] = new MGridColumn(_M('Nota final', $module), 'center', true, NULL, true, NULL, false);
        $columns[] = new MGridColumn(_M('Situao', $module), 'center', true, NULL, true, NULL, false);
        
        parent::__construct($filters, $columns, __CLASS__);

        $this->actions = array();

        $href = SForm::getPopupLink(array('args' => array('ofertaDoResidenteId' => '%0%','ofertaDeUnidadeTematicaId' => '%1%', 'residenteId' => '%2%'), 'method' => 'showInformation', 'encodeArgs' => false));
        $this->addActionIcon(_M('Detalhes de carga horaria', $module), 'find.png', $href);
    }
}
