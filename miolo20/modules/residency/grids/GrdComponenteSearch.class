class GrdComponenteSearch extends SGrid
{

    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'rescomponente.componenteid' => new MGridColumn(_M('Cdigo', $module)),
            'rescomponente.descricao' => new MGridColumn(_M('Descrio', $module)),
        );

        parent::__construct($filters, $columns, __CLASS__, array( 'componenteid' => '%rescomponente.componenteid%' ));
    }

}

