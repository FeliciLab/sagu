class GrdResidenteCargaHoraria extends SGrid
{
    public function __construct($filters)
    {
        $module = SAGU::getFileModule(__FILE__);

        $columns = array();
        $columns[] = new MGridColumn(_M('Tipo', $module), 'left', true, null, true, null, true);
        $columns[] = new MGridColumn(_M('Carga horaria presente do residente (h)', $module), 'center', true, null, true, null, true);
        $columns[] = new MGridColumn(_M('Carga horaria lanada (h)', $module), 'center', true, null, true, null, true);
        $columns[] = new MGridColumn(_M('Frequncia mnima (%)', $module), 'center', true, null, true, null, true);
        $columns[] = new MGridColumn(_M('Carga horaria P/aprovao (h)', $module), 'center', true, null, true, null, true);
        $columns[] = new MGridColumn(_M('Carga horaria P/faltas (h)', $module), 'center', true, null, true, null, true);

        parent::__construct($filters, $columns, __CLASS__);

    }
}
