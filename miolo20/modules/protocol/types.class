/**
 * Basic database classes
 */


class PtcSubjectSector extends BasLog
{
    public $subjectId;
    public $sectorId;
    public $level;
    public $maxDays;
    public $maxTime;
    public $paraCoordenador;
    public $permiteFecharProtocolo;
}

class PtcRequiredDocument extends BasLog
{
    public $requiredDocumentId;
    public $subjectId;
    public $description;
}

class PtcRequestStatus extends Baslog
{
    public $requestStatusId;
    public $description;
}

class PtcRequest extends BasLog
{
    const STATUS_OPEN = 1;
    const STATUS_RUNNING = 2;
    const STATUS_CLOSED = 3;
    const STATUS_CANCELLED = 4;

    public $requestId; 
    public $personId;
    public $sectorId;
    public $subjectId; 
    public $statusId; 
    public $currentSubjectId; 
    public $currentSectorId; 
    public $currentLevel; 
    public $number; 
    public $description; 
    public $startTime; 
    public $endTime;
    public $courseId;
    public $courseVersion;
    public $turnId;
    public $unitId;
}

class PtcDispatch extends BasLog
{
    public $requestId; 
    public $dateTime; 
    public $subjectId; 
    public $sectorId; 
    public $level; 
    public $forwardedSubjectId; 
    public $forwardedSectorId; 
    public $forwardedLevel; 
    public $description; 
}

class PtcDocument extends BasLog
{
    public $documentId;
    public $requestId;
    public $description;
    public $fileId;
}
