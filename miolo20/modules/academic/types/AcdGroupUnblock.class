/**
 * @access public
 * @package Basic
 */
class AcdGroupUnblock extends SType
{
    protected $_utilizaNovoPadrao = true;
 
    public $groupunblockid;
    public $groupId;
    public $limitdate;
    public $registerdate;
    public $observation;
    
    public function obterHistoricoParaTableRaw($groupId)
    {
        $data = array();
        $rows = $this->findMany( $this->msql()->addEqualCondition('acdgroupunblock.groupid', $groupId)->setOrderBy('acdgroupunblock.registerdate DESC') );

        foreach ( $rows as $row )
        {
            $row instanceof AcdGroupUnblock;
            
            $data[] = array($row->registerdate, $row->limitdate, $row->observation);
        }
        
        return $data;
    }
}
