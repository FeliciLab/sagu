class FrmMensagemDeAvaliacaoDosAlunosBusca extends SSearchForm
{
    
    public function __construct()
    {
        $module = MIOLO::getCurrentModule();
        
        parent::__construct(_M('Mensagem de avaliao dos alunos', 'basic'),new AcdMensagemDeAvaliacaoDosAlunos(), array('mensagemDeAvaliacaoDosAlunosId'), 'BusMensagemDeAvaliacaoDosAlunos');
    }
    
    public function defineFields()
    {
        $MIOLO = MIOLO::getInstance();
        $module = MIOLO::getCurrentModule();
        $action = MIOLO::getCurrentAction();
        
        $mensagemDeAvaliacaoDosAlunosIdS = new MTextField('mensagemDeAvaliacaoDosAlunosIdS', $this->getFormValue('mensagemDeAvaliacaoDosAlunosIdS', $data->mensagemDeAvaliacaoDosAlunosIdS), _M('Cdigo', $module), SAGU::getParameter('BASIC', 'FIELD_ID_SIZE'));
        $mensagemDeAvaliacaoDosAlunosIdS->setJsHint(_M('Informe o cdigo da mensagem de avaliao dos alunos', $module));
        $fields[] = $mensagemDeAvaliacaoDosAlunosIdS;
        $mensagemS = new MTextField('mensagemS', $this->mensagemS->value, _M('Mensagem', $module), SAGU::getParameter('BASIC', 'FIELD_DESCRIPTION_SIZE'));
        $mensagemS->setJsHint(_M('Informe a mensagem de avaliao dos alunos',$module));
        $fields[] = $mensagemS;
        
        parent::defineFields($fields, $module, 'GrdMensagemDeAvaliacaoDosAlunos');
    }
}
