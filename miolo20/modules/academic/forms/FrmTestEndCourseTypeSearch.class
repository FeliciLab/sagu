/**
 * Form to search the basTestEndCourseType table
 */
class FrmTestEndCourseTypeSearch extends SSearchForm
{
    /**
     * Class constructor
     */
    public function __construct($data)
    {
        parent::__construct(_M('Localizar', 'basic'), new AcdTestEndCourseType(), array('testEndCourseTypeId'), 'BusTestEndCourseType');
    }

    /**
     *  Default method to define fields
     */
    public function defineFields()
    {
        $MIOLO = MIOLO::getInstance();
        $module = SAGU::getFileModule(__FILE__);
        $action = MIOLO::getCurrentAction();

        // Field test end course type
        $testEndCourseTypeIdS = new MTextField('testEndCourseTypeIdS', $this->getFormValue('testEndCourseTypeIdS', $data->testEndCourseTypeIdS), _M('Cdigo', $module), SAGU::getParameter('BASIC', 'FIELD_ID_SIZE'));
        $testEndCourseTypeIdS->setJsHint(_M('Informe o cdigo do tipo de avaliao do MEC', $module));
        $fields[] = $testEndCourseTypeIdS;
        $descriptionS = new MTextField('descriptionS', $this->descriptionS->value, _M('Descrio', $module), SAGU::getParameter('BASIC', 'FIELD_DESCRIPTION_SIZE'));
        $descriptionS->setJsHint(_M('Informe a descrio do tipo de avaliao do MEC',$module));
        $fields[] = $descriptionS;

        parent::defineFields($fields, $module, 'GrdTestEndCourseTypeSearch', $validators);
    }
}
