class GrdEnrollConfig extends SGrid
{
    public function __construct($filters)
    {
        $MIOLO = MIOLO::getInstance();
        $module = SAGU::getFileModule(__FILE__);
        
        $columns = array( 
            new MGridColumn( _M('Cdigo da configurao', $module), 'left', true, null, false, null, true ),
            new MGridColumn( _M('Data inicial', $module), 'left',  true, null, true, null, true ),
            new MGridColumn( _M('Data final', $module), 'left',  true, null, true, null, true ),
            new MGridColumn( _M('Cdigo do curso', $module), 'left', true, null, true, null, true ),
            new MGridColumn( _M('Nome do curso', $module), 'left',  true, null, true, null, true ),
            new MGridColumn( _M('Verso', $module), 'left',  true, null, true, null, true ),
            new MGridColumn( _M('Turno', $module), 'left',  true, null, true, null, true ),
            new MGridColumn( _M('Unidade', $module), 'left',  true, null, true, null, true ),
        );
        
        parent::__construct($filters, $columns, __CLASS__, array('enrollconfigid' => '%0%'));
    }
}
