class GrdMatriculaEmLote extends SGrid
{
    /**
     * Class constructor
     **/
    public function __construct($filters)
    {
        $MIOLO = MIOLO::getInstance();
        $action = MIOLO::getCurrentAction();
        $module = SAGU::getFileModule(__FILE__);

        $columns = array(
            new MGridColumn(_M('groupId_curriculumid', $module), 'right', true, null, false),
            new MGridColumn(_M('Cod. oferecida', $module), 'right', true, null, true),
            new MGridColumn(_M('Cod. disciplina', $module), 'left', true, null, true),
            new MGridColumn(_M('Versao disciplina', $module), 'right', true, null, false),
            new MGridColumn(_M('Disciplina', $module), 'left', true, null, true),
            new MGridColumn(_M('Semestre', $module), 'right', true, null, true),
            new MGridColumn(_M('Curriculumid', $module), 'right', true, null, false),
            new MGridColumn(_M('Crditos', $module), 'right', true, null, true),
            new MGridColumn(_M('Carga horria', $module), 'right', true, null, true),
            new MGridColumn(_M('Data inicial', $module), 'right', true, null, true),
            new MGridColumn(_M('Data final', $module), 'right', true, null, true),
            new MGridColumn(_M('Turma', $module), 'right', true, null, true),
            new MGridColumn(_M('Professores', $module), 'right', true, null, true),            
            new MGridColumn(_M('Vagas utilizadas', $module), 'right', true, null, true),
        );

        parent::__construct($filters, $columns, __CLASS__);

        $this->showExport = false;
        $this->disablePagination();
        $this->disableColumnSorting();
    }
}

