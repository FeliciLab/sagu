class GrdConceptGradeSearch extends SGrid
{
    /*
     * Construtor da classe
     */
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        /*
         * MGridColumn constructor parameters:
         *    $title = ''
         *    $align = 'left'
         *    $nowrap = false
         *    $width = 0
         *    $visible = true
         *    $options = null
         *    $order = false
         *    $filter = false
         */
        $columns[] = new MGridColumn(_M('Cdigo', $module), 'right', true, null, true, null, true);
        $columns[] = new MGridColumn(_M('Descrio', $module), 'left', true, null, true, null, true);

        $primaryKey = array('conceptGroupId' => '%0%');

        parent::__construct($filters, $columns, 'GrdConceptGradeSearch', $primaryKey);
    }
}
