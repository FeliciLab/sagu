class GrdRegimeDomiciliarSearch extends SGrid
{
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'acdregimedomiciliar.regimedomiciliarid' => new MGridColumn( _M('Cdigo', $module)),            
            'acdregimedomiciliar.enrollid' => new MGridColumn( _M('Matrcula', $module)),
            'acdregimedomiciliar.datainicial' => new MGridColumn( _M('Data Inicial', $module)),
            'acdregimedomiciliar.datafinal' => new MGridColumn( _M('Data Final', $module)),
            'acdregimedomiciliar.justificativa' => new MGridColumn( _M('Justificativa', $module) ),
        );

        parent::__construct($filters, $columns, __CLASS__, array('regimedomiciliarid' => '%acdregimedomiciliar.regimedomiciliarid%'));
    }
}
