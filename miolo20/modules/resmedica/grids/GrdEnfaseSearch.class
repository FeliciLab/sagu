class GrdEnfaseSearch extends SGrid
{
    public function __construct($filters)
    {
        $module = MIOLO::getCurrentModule();
        $columns[] = new MGridColumn(_M('Cdigo', $module), 'right', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Descrio', $module), 'left', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Abreviatura', $module), 'left', true, NULL, true, NULL, true);
        $columns[] = new MGridColumn(_M('Responsvel', $module), 'left', true, NULL, true, NULL, true);
        $primaryKeys = array('enfaseId'=>'%0%', );
        parent::__construct($filters, $columns, __CLASS__, $primaryKeys);
    }
}


