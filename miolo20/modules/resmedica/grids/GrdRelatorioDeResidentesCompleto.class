    {
        $module = SAGU::getFileModule(__FILE__);

        // Colunas
        $columns = array();
        $columns[] = new MGridColumn( _M('Nome', $module), 'left',  true, null, true, null, false );
        $columns[] = new MGridColumn( _M('Profisso', $module), 'left', true, null, true, null, false );
        $columns[] = new MGridColumn( _M('Registro profissional', $module), 'right', true, null, true, null, false );
        $columns[] = new MGridColumn( _M('Situao', $module), 'left', true, null, true, null, false );
        $columns[] = new MGridColumn( _M('Matrcula', $module), 'left', true, null, true, null, false );
