class GrdRelatorioDeResidentesVagas extends SGrid
{
    public $colP1;
    public $colP2;
    public $colP3;

    public function __construct($filters)
    {
        $module = SAGU::getFileModule(__FILE__);

        // Colunas
        $columns = array();
        $columns[] = new MGridColumn( _M('nfase'), 'left',  true, null, true, null, false );
        $columns[] = $this->colP1 = new MGridColumn( _M('P1', $module), 'right', true, null, true, null, false );
        $columns[] = $this->colP2 = new MGridColumn( _M('P2', $module), 'right', true, null, true, null, false );
        $columns[] = $this->colP3 = new MGridColumn( _M('P3', $module), 'right', true, null, true, null, false );
        $columns[] = new MGridColumn( _M('Total de residentes', $module), 'right', true, null, true, null, false );

        parent::__construct($filters, $columns, __CLASS__, $primaryKey);

        // Remove as aes da Grid
        $this->clearActions();
    }
}
