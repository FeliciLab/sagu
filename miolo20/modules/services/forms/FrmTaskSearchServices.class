/**
 * Form to search the tasks
 */
class FrmTaskSearchServices extends FrmTaskSearch
{
    /**
     * Class constructor
     */
    public function __construct()
    {
        parent::__construct();
    }

    /**
     * Default method to define fields
     */
    public function createFields()
    {
        parent::createFields();
    }

    /**
     * Obtm a grid de pendncias do mdulo servios
     */
    public function getGrid($filters)
    {
        $MIOLO = MIOLO::getInstance();
        $module = SAGU::getFileModule(__FILE__);
        $grid = $MIOLO->getUI()->getGrid($module, 'GrdTaskSearchServices', $filters);
        return $grid;
    }
}
